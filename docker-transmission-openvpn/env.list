OPENVPN_PROVIDER=NORDVPN
OPENVPN_USERNAME=<VPN USERNAME>
OPENVPN_PASSWORD=<VPN PASSWORD>
OPENVPN_OPTS=--inactive 3600 --ping 10 --ping-exit 60 --mssfix 1452 --mute-replay-warnings
NORDVPN_COUNTRY=US
NORDVPN_CATEGORY=P2P
WEBPROXY_ENABLED=false
LOCAL_NETWORK=192.168.0.0/24
TRANSMISSION_WEB_UI=flood-for-transmission
TRANSMISSION_SPEED_LIMIT_UP=1024
TRANSMISSION_SPEED_LIMIT_UP_ENABLED=true
TRANSMISSION_ALT_SPEED_UP=256
TRANSMISSION_ALT_SPEED_DOWN=1024
TRANSMISSION_LPD_ENABLED=true
TRANSMISSION_RPC_AUTHENTICATION_REQUIRED=true
TRANSMISSION_RPC_USERNAME=<TRANSMISSION_RPC_USERNAME>
TRANSMISSION_RPC_PASSWORD=<TRANSMISSION_RPC_PASSWORD>
TRANSMISSION_RATIO_LIMIT_ENABLED=true
TRANSMISSION_RATIO_LIMIT=1.5
